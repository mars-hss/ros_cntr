# configuration of ros_docker

ROS_DISTRO="noetic"         # Default ros version
bundles="desktop"           # ROS PKG desktop / base / core
ws_name_tag="_ws"           # _ws for workspace
gpu="nvidia"                # Change gpu resource from ( nvidia/intel/amd )
port=false		            # true -> Open port for unity connection
multi_distro=false          # multi-dockers for same ros version.
multi_ws=false              # multi-dockers for same ros and workspace.
